{
  "mode": "train",
  "seed": 1,
  "data_augment": true,
  "dataset": "imagenet",
  "optimizer": "adamw",
  "learning_rate": 0.002,
  "weight_decay": 0.001,
  "grad_clip": 1.0,
  "lr_scheduler": "cosineannealinglr",
  "num_epochs": 20,
  "warmup_epochs": 2,
  "log_interval": 10,
  "checkpoint_interval": 5,
  "batchsize": 256,
  "num_workers": 16,
  "use_amp": true,
  "log_checkpoints": false,
  "checkpoint_dir": "imagenet_mini_base",
  "use_wandb": false,
  "pca_labels": false,
  "pca_n_classes": 32,
  "pca_labels_folder": "pca_labels_alexnet",
  "model_class": "custom_model",
  "custom_model": {
    "model_name": "CustomCNN",
    "arch": {
      "nonlinearity": "relu",
      "conv_trainable": "11111",
      "fc_trainable": "111",
      "pooling_type": "max",
      "dropout": 0.5,
      "batchnorm": true
    }
  },
  "standard_model": {
    "model_name": "AlexNet",
    "pretrained_dataset": "none"
  }
}